{
    "env": {
        "node": true,
        "browser": true,
        "webextensions": true,
        "jest/globals": true
    },
    "plugins": [
        "jest"
    ],
    "extends": [
        "eslint:recommended",
        "google",
        "prettier"
    ],
    "rules": {
        "require-jsdoc": "off"
    },
    "parserOptions": {
        "ecmaVersion": 2017
    }
}