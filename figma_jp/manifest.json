{
  "manifest_version": 2,
  "name": "Figma 日本語化",
  "version": "0.3.12",
  "description": "Figma の表示を日本語化する拡張です。",
  "homepage_url": "https://github.com/junkawa/figma_jp",
  "content_scripts": [
    {
      "matches": ["https://www.figma.com/*"],
      "js": ["lang.js", "content.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": []
}
